msgstr ""

#: ../../user/playlists/content/reorder.md:3
msgid "You can change the order of the tracks in a playlist. To do this:"
msgstr ""

#: ../../user/playlists/content/reorder.md:0
msgid "Desktop"
msgstr ""

#: ../../user/playlists/content/reorder.md:11
#: ../../user/playlists/content/reorder.md:23
msgid "Log in to your account."
msgstr ""

#: ../../user/playlists/content/reorder.md:12
msgid "Select {guilabel}`Playlists` under the {guilabel}`My Library` section of the sidebar. The {guilabel}`Browsing playlists` screen opens."
msgstr ""

#: ../../user/playlists/content/reorder.md:13
#: ../../user/playlists/content/reorder.md:26
msgid "Select {guilabel}`Manage your playlists`. The {guilabel}`Manage playlists` screen appears."
msgstr ""

#: ../../user/playlists/content/reorder.md:14
#: ../../user/playlists/content/reorder.md:27
msgid "Select the edit icon ({fa}`edit`) next to the playlist you want to edit. The playlist edit screen appears."
msgstr ""

#: ../../user/playlists/content/reorder.md:15
#: ../../user/playlists/content/reorder.md:28
msgid "Drag and drop the tracks in the playlist into the order you want. The position number of each track appears on the left side."
msgstr ""

#: ../../user/playlists/content/reorder.md:16
#: ../../user/playlists/content/reorder.md:29
msgid "Select {guilabel}`Stop Editing` to finish editing."
msgstr ""

